{
  "manifest_version": 2,

  "name": "CodeBreaker",
  "description": "Enhance season 2 of breakthecode.tech",
  "version": "0.0.1",

  "content_scripts": [
      {
          "matches": ["https://breakthecode.tech/game"],
          "js": ["js/vendor.js", "js/content_script.js"]
      }
  ],

  "web_accessible_resources": ["js/injected.js", "hackedFlowers.js", "xp.css"],

  "permissions": [
    "https://breakthecode.tech/game",
    "*://facebook.com/*",
    "*://facebook.net/*",
    "*://*.facebook.com/*",
    "*://analytics.tiktok.com/*",
    "https://techdomains.piwik.pro/ppms.js",
    "webRequest",
    "webRequestBlocking"
  ],
  "background": {
    "scripts": ["js/background_script.js"]
  }
}
